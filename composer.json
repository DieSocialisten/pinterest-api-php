{
    "name": "diesocialisten/pinterest-api-php",
    "description": "A PHP wrapper for the official Pinterest API v3 tailored to walls.io needs. Fork of dirkgroenen/pinterest-api-php",
    "authors": [
        {
            "name": "Dirk Groenen",
            "email": "dirk@bitlabs.nl",
            "homepage": "http://dirkgroenen.nl",
            "role": "Original Creator/Developer"
        }
    ],
    "license": "Apache-2.0",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*",
        "vlucas/phpdotenv": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "DirkGroenen\\Pinterest\\": "src/Pinterest"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DirkGroenen\\Pinterest\\Tests\\": "tests/Pinterest"
        }
    }
}
